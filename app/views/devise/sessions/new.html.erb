<h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field lala">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, placeholder: "e-mail", autocomplete: "email" %>
  </div>

  <div class="field lala">
    <%= f.label :password %><br />
    <%= f.password_field :password, placeholder: "Password", autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field shuo">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
